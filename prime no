print("check if  NUMBER is PRIME NUMBER or not")
n = int(input("enter the  number;>"))


if n > 1:
    for i in range(2, n):
        if (n % i) != 0:
            print(f"{n} is a prime number")
            break
        else:
            print(f"{n} is not a prime number")
            break
else:
    print("number is not a prime number")
